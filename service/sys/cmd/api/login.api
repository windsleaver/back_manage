syntax = "v1"

info(
    title: "type title here"
    desc: "type desc here"
    author: "type author here"
    email: "type email here"
    version: "type version here"
)

type LoginReq {
    UserName string `json:"userName"`
    Password string `json:"password"`
}

type LoginResp {
    ID string `json:"id"`
    UserName string `json:"userName"`
}

service sys-api {
    @handler login
        post /user/login (LoginReq) returns (LoginResp)
}
